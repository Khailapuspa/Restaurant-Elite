{
    "env": {
      "es2021": true,
      "browser": true,
      "serviceworker": true
    },
    "globals": {
      "navigator": "readonly",
      "self": "readonly"  // Tambahkan self sebagai variabel global yang diizinkan
    },
    "extends": ["airbnb-base"],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "no-underscore-dangle": "off",
      "no-restricted-globals": "off", // Disable this rule completely
      "import/extensions": [
        "error",
        "always",
        {
          "js": "never",
          "json": "never"
        }
      ],
      "import/no-unresolved": "off",
      "no-unused-vars": "off" // Disable unused vars rule
    }
  }
